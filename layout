	CHR_LV1CA: start=$1000, size=$1000, fill=yes, fillval=$FF, bank=$C0; # Level 1 Complete A
	CHR_LV1CB: start=$1000, size=$1000, fill=yes, fillval=$FF, bank=$C4; # Level 1 Complete B
