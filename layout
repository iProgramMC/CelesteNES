	CHR_SPHRT: start=$1C00, size=$0400, fill=yes, fillval=$FF, bank=$FB; # Crystal Heart Sprite
	CHR_DGRAN: start=$1000, size=$1000, fill=yes, fillval=$FF, bank=$FC; # Granny dialog portraits
}

SEGMENTS
{
